<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="overview-wrap">
                        <h2 class="title-1 p-2">Reports</h2>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="card-title">
                                <h3 class="text-center title-2">Update the Station</h3>
                            </div>
                            <hr>
                            {{> messages}}
                            <form action="/admin/dashboard/reports" method="POST">
                                <div class="row">
                                    <div class="col-6">
                                        <label for="tableModel" class="form-control-label">Table date of:</label>
                                        <div class="form-group">
                                            <select name="tableModel" id="tableModel" class="form-control" required>
                                            {{#each data.tableList}}
                                                <option value="{{value}}">{{title}}</option>
                                            {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <label for="basedOn" class="form-control-label">Based on:</label>
                                        <div class="form-group">
                                            <select name="basedOn" id="basedOn" class="form-control" required>
                                            {{#each data.basedOnList}}
                                                <option value="{{value}}">{{title}}</option>
                                            {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                {{!-- <div class="row">
                                    <div class="col">
                                        <label for="cc-exp" class=" form-control-label">Station Name</label>
                                        <div class="form-group">
                                            <input id="cc-exp" name="stationName" type="text"
                                                class="form-control cc-exp" value="{{data.updateStation.stationName}}"
                                                placeholder="Name" required>
                                        </div>
                                    </div>
                                </div> --}}
                                <div>
                                    <button id="payment-button" type="submit"
                                        class="btn btn-lg btn-info btn-block">QUERY</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="overview-wrap">
                        <h2 class="title-1">Users Data Table</h2>
                    </div>
                </div>
                <div class="col-md-12">
                    <!-- DATA TABLE-->
                    <div class="table-responsive m-b-40">
                        <table class="table table-borderless table-data3">
                            <thead>
                                <tr>
                                    <th>S No.</th>
                                    <th>Timestamp</th>
                                    <th>Name</th>
                                    <th>Address</th>
                                    <th>Age</th>
                                    <th>Gender</th>
                                    <th>Contact</th>
                                    <th>Email</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>1</td>
                                    <td>2018-09-29 05:57</td>
                                    <td>Name</td>
                                    <td>Address</td>
                                    <td>Age</td>
                                    <td>Gender</td>
                                    <td>Contact</td>
                                    <td>Email</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <!-- END DATA TABLE-->
                </div>
                {{>footer}}
            </div>
        </div>
    </div>